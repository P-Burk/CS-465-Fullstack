<!DOCTYPE html>
<!-- Website template by freewebsitetemplates.com -->
<html>
<head>
	<meta charset="UTF-8">
	<title>Rooms - Travlr Getaways Website Template</title>
	<link rel="stylesheet" href="css/style.css" type="text/css">
</head>
<body>
	<div id="background">
		<div id="page">
			{{> header}}
			<div id="contents">
				<div class="box">
					<div>
						<div class="body">
							<h1>Rooms</h1>
							<ul id="rooms">
			{{#each roomInfo}}
								<li>
									<a href="rooms.html"><img src="images/{{this.roomImage}}" alt="Img"></a>
									<h2><a href="rooms.html">{{this.roomType}}</a></h2>
									<p>
										{{this.roomDescription}}
									</p>
									<span class="rate">{{this.roomRate}}</span>
								</li>
			{{/each}}
							</ul>
						</div>
					</div>
				</div>
			</div>
		</div>
		{{> footer}}
	</div>
</body>
</html>